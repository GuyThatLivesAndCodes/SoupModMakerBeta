{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:path:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-write-text-file",
    "fs:allow-read-text-file",
    "fs:allow-read-dir",
    "fs:allow-create-dir",
    "fs:allow-exists",
    "fs:allow-stat",
    "fs:default"
  ]
}

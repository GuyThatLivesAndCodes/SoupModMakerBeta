{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "core:path:default",
    "dialog:allow-open",
    "dialog:allow-save",
    "fs:default",
    "fs:read-all",
    "fs:write-all"
  ],
  "scope": {
    "allow": [
      "**"
    ]
  }
}
